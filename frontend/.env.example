# [EDIT] - 2024-01-15 - Created frontend environment variables example - Ediens Team

# API Configuration
VITE_API_URL=http://localhost:3001
VITE_API_TIMEOUT=10000

# OAuth Configuration
VITE_GOOGLE_CLIENT_ID=your-google-client-id
VITE_FACEBOOK_APP_ID=your-facebook-app-id

# Map Configuration
VITE_MAPBOX_ACCESS_TOKEN=your-mapbox-access-token
VITE_MAPBOX_STYLE_URL=mapbox://styles/mapbox/streets-v11

# Feature Flags
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG_LOGGING=true

# App Configuration
VITE_APP_NAME=Ediens
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Food sharing platform to reduce waste in Latvia

# External Services
VITE_CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
VITE_CLOUDINARY_UPLOAD_PRESET=ediens_uploads

# Development Configuration
VITE_DEV_MODE=true
VITE_MOCK_API=false
VITE_ENABLE_HOT_RELOAD=true